[include]
	path = ~/.dotfiles/local/.gitconfig.local
[core]
	excludesfile = ~/.gitignore_global
	autocrlf = input
	safecrlf = true
[alias]
	graph = log --oneline --abbrev-commit --graph --decorate --color
	co = checkout
[merge]
	tool = vimdiff
[init]
	templatedir = ~/.git_template
	defaultBranch = main
