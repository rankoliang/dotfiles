{
  "suggest.echodocSupport": true,
  "coc.preferences.colorSupport": true,
  "coc.preferences.formatOnInsertLeave": true,
  "coc.preferences.rootPatterns": [".git", ".hg", ".projections.json"],
  "css.completion.completePropertyWithSemicolon": true,
  "css.validate": true,
  "css.lint.unknownAtRules": "ignore",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.errorSign": "✘",
  "diagnostic.infoSign": "i",
  "diagnostic.messageTarget": "echo",
  "diagnostic.warningSign": "△",
  "eslint.quiet": true,
  "eslint.autoFixOnSave": true,
  "html.enable": true,
  "prettier.printWidth": 120,
  "scss.validate": true,
  "snippets.extends": {
    "scss": ["css"]
  },
  "snippets.priority": 200,
  "snippets.ultisnips.enable": false,
  "solargraph.completion": true,
  "solargraph.diagnostics": true,
  "solargraph.folding": true,
  "solargraph.formatting": true,
  "solargraph.hover": true,
  "solargraph.rename": true,
  "solargraph.symbols": true,
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.headwind.runOnSave": true
}
